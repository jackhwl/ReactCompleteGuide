                var warningCounter = $('div.mi-icon.text-warning').length;
                var dangerCounter = $('div.mi-icon.text-danger').length;

	        	$("#liNotifications").removeClass("hidden");
	        	
	        	if (count == "0") {
	        		ClearAllMessages();
	        		return;
