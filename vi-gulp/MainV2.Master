	        		message.html(script);

                    $("#labelMessagesCount").html(String(Number(count) + 1));
                    $("#labelMessagesCount2").html(String(Number(count) + 1) + (Number(count) + 1 > 1 ? " Messages" : " Message"));

	        		count = String(Number(count) + 1);
